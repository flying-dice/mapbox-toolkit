{
  "name": "@flying-dice/mapbox-toolkit",
  "version": "1.0.0",
  "description": "Mapbox Toolkit wrapping mapbox-gl and @mapbox/mapbox-gl-draw",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "prettier": "prettier ./src --write",
    "lint": "eslint ./src --fix",
    "test": "jest",
    "build": "rimraf dist && tsc",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flying-dice/war-room-models.git"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.4.1",
    "@types/mapbox-gl": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:unicorn/recommended"
    ],
    "parser": "@typescript-eslint/parser"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.3.0",
    "@turf/turf": "^6.5.0",
    "@types/mapbox__mapbox-gl-draw": "^1.2.3",
    "debug": "^4.3.4",
    "mapbox-gl": "^2.8.2",
    "uuid": "^8.3.2"
  }
}
